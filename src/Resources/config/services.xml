<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>

        <service id="AgeChecker\Service\AgeCheckerClient">
            <argument type="service" id="Psr\Log\LoggerInterface"/>
        </service>

        <service id="AgeChecker\Storefront\Controller\AgeCheckerController" autowire="true" public="true">
            <argument type="service" id="customer.repository"/>
            <call method="setContainer">
                <argument type="service" id="service_container"/>
            </call>
            <call method="setTwig">
                <argument type="service" id="twig"/>
            </call>
            <argument type="service" id="AgeChecker\Service\AgeCheckerClient"/>
            <argument type="service" id="twig"/>
            <argument type="service" id="service_container"/>
        </service>

        <service id="AgeChecker\Subscriber\CheckoutSubscriber">
            <argument type="service" id="Shopware\Core\System\SystemConfig\SystemConfigService" />
            <tag name="kernel.event_subscriber"/>
        </service>

        <service id="AgeChecker\Service\CustomFieldsInstaller">
            <argument type="service" id="custom_field_set.repository"/>
            <argument type="service" id="custom_field_set_relation.repository"/>
            <argument type="service" id="custom_field.repository"/>
        </service>

    </services>
</container>
