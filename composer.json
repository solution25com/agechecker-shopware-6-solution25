{
    "name": "s25/age-checker",
    "description": "Age Checker - Shopware 6 Plugin",
    "type": "shopware-platform-plugin",
    "version": "1.0.0",
    "license": "MIT",
    "authors": [
        {
            "name": "solution25"
        }
    ],
    "require": {
        "shopware/core": "~6.6.0",
        "shopware/storefront": "^6.6"

    },
    "extra": {
        "shopware-plugin-class": "AgeChecker\\AgeChecker",
        "supportLink": {
            "de-DE": "https://solution25.com/",
            "en-GB": "https://solution25.com/en/home_en/"
        }
    },
    "autoload": {
        "psr-4": {
            "AgeChecker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AgeChecker\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/runtime": true
        }
    }
}
